{
  "name": "react-tsoa-server-boilerplate",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist",
    "start:dev": "concurrently -n TSOA,API -c blue,yellow npm:tsoa:watch nodemon",
    "build": "tsoa spec-and-routes && tsc",
    "tsoa:watch": "nodemon -w src/**/*.controller.* -x tsoa spec-and-routes --ext .ts --ignore src/routes.ts",
    "build:watch": "tsc --watch --preserveWatchOutput --incremental",
    "test": "DOTENV_CONFIG_PATH=.env.test mocha",
    "test:cy": "nyc npm run test",
    "lint": "eslint --cache"
  },
  "nyc": {
    "include": "src/**",
    "extension": ".ts",
    "reporter": [
      "text",
      "html"
    ]
  },
  "nodemon": {
    "exec": "ts-node src",
    "watch": [
      "src"
    ],
    "ext": "ts"
  },
  "dependencies": {
    "body-parser": "1.20.0",
    "cookie-parser": "1.4.6",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "express-correlation-id": "2.0.1",
    "helmet": "5.1.0",
    "inversify": "6.0.1",
    "inversify-binding-decorators": "4.0.0",
    "jose": "4.8.3",
    "lodash": "4.17.20",
    "mongoose": "6.4.4",
    "morgan": "1.10.0",
    "reflect-metadata": "0.1.13",
    "source-map-support": "0.5.21",
    "supertest": "6.2.4",
    "tsoa": "4.1.0",
    "uuid": "8.3.2",
    "winston": "3.8.1"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/chai": "4.3.1",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.9",
    "@types/express-correlation-id": "1.2.4",
    "@types/lodash": "4.14.182",
    "@types/mocha": "^9.1.1",
    "@types/morgan": "1.9.2",
    "@types/node": "18.0.3",
    "@types/sinon": "10.0.12",
    "@types/sinon-chai": "3.2.8",
    "@types/supertest": "2.0.12",
    "@types/uuid": "8.3.4",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "axios": "0.27.2",
    "chai": "4.3.6",
    "concurrently": "7.2.2",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "mocha": "10.0.0",
    "mongodb": "4.7.0",
    "mongodb-memory-server": "8.7.2",
    "nodemon": "2.0.19",
    "nyc": "15.1.0",
    "prettier": "2.7.1",
    "sinon": "14.0.0",
    "ts-node-dev": "2.0.0",
    "typescript": "4.7.4"
  }
}
