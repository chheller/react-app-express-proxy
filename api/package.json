{
  "name": "react-tsoa-server-boilerplate",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist",
    "start:dev": "concurrently -n TSOA,SERVER -c blue,green npm:watch-tsoa \"ts-node-dev --respawn src\"",
    "build": "tsoa spec-and-routes && tsc",
    "watch-tsoa": "nodemon -w src -x tsoa spec-and-routes",
    "test": "nyc jest"
  },
  "nyc": {
    "include": "src/**",
    "extension": ".ts",
    "reporter": [
      "text",
      "html"
    ]
  },
  "dependencies": {
    "@types/jest": "^27.0.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "inversify": "^6.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "lodash": "^4.17.20",
    "mongoose": "^6.0.12",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "tsoa": "^3.4.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.9",
    "@types/lodash": "^4.14.165",
    "@types/morgan": "^1.9.2",
    "@types/node": "^16.11.4",
    "@types/uuid": "^8.3.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "axios": "^0.23.0",
    "concurrently": "^6.3.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.3.1",
    "mongodb-memory-server": "^7.5.1",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.2"
  }
}
